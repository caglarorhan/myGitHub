{
  "name": "My GitHub",
  "description" : "Some of owners casual github informations",
  "version": "2019.0.2",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "My GitHub",
    "default_icon": "./img/mygithub_16.png"
  },
  "permissions": ["declarativeContent","storage","notifications"],
  "icons": {
    "16": "./img/mygithub_16.png",
    "32": "./img/mygithub_32.png",
    "64": "./img/mygithub_64.png",
    "128": "./img/mygithub_128.png",
    "256": "./img/mygithub_256.png",
    "512": "./img/mygithub_512.png"
  },
  "background" : { "scripts" : ["./js/background.js"] },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Some of owners casual github informations"
    }
  },
  "options_page" : "options.html"

}
